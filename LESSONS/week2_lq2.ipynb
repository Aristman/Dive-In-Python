   "source": [
    "from random import randint\n",
    "\n",
    "random_number = randint(1,100)\n",
    "random_set = set()\n",
    "while random_number not in random_set:\n",
    "    random_set.add(random_number)\n",
    "    random_number = randint(1,100)\n",
    "\n",
    "print('Повтор через {} итераций'.format(len(random_set)))"
   ]
